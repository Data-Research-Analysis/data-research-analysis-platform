{
  "name": "backend-marketing",
  "version": "1.0.0",
  "description": "Backend server for the marketing website.",
  "scripts": {
    "dev": "node --import tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js __tests__",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js integration",
    "test:ratelimit": "node --experimental-vm-modules node_modules/jest/bin/jest.js rateLimit",
    "test:fast": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns=article-markdown --testPathIgnorePatterns=integration",
    "test:verbose": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run -- -d ./src/datasources/PostgresDSMigrations.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/datasources/PostgresDSMigrations.ts ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm migration:create -- -d ./src/datasources/PostgresDSMigrations.ts ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/datasources/PostgresDSMigrations.ts",
    "migrate:articles-markdown": "node --import tsx src/utilities/MigrateArticlesMarkdown.ts",
    "seed:run": "tsx ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@sinonjs/fake-timers": "^15.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.6",
    "@types/nodemailer": "^6.4.19",
    "@types/nodemailer-smtp-transport": "^2.7.8",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "node-pg-migrate": "^7.9.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/client-textract": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@google-analytics/data": "^5.2.1",
    "@google/genai": "^1.30.0",
    "@jorgebodega/typeorm-seeding": "^7.1.0",
    "@types/json2csv": "^5.0.7",
    "@types/node-fetch": "^2.6.13",
    "@types/ws": "^8.18.1",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "googleapis": "^168.0.0",
    "install": "^0.13.0",
    "ioredis": "^5.8.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "node-xlsx": "^0.24.0",
    "nodemailer": "^6.10.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm": "^11.6.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "theta-mn-queue": "^3.0.9",
    "turndown": "^7.2.2",
    "typeorm": "^0.3.27",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  }
}
